{
  "name": "@verypossible/eslint-config",
  "version": "1.1.1",
  "main": "index.js",
  "files": [
    "index.js",
    "react.js",
    "lib"
  ],
  "author": "Sara Pope <sara@verypossible.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/verypossible-labs/eslint-config-very"
  },
  "bugs": {
    "url": "https://github.com/verypossible-labs/eslint-config-very/issues"
  },
  "license": "MIT",
  "scripts": {
    "semantic-release": "semantic-release",
    "commit": "git-cz",
